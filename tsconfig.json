{
    "compileOnSave": false,
    "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "lib": [
    "dom",
    "es2018"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "typeRoots": [
    "./node_modules/@types"
    ]
    },
    "exclude": [
    "node_modules",
    "dist"
    ]
}